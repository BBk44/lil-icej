javascript:import { saveCharacter } from "../utils/util";    export default class Hack { public element: HTMLButtonElement; public name: string; // @ts-expect-error private description: String;  constructor( public parent: HTMLDivElement, name?:%20string,%20description?:%20string%20)%20{%20this.name%20=%20%22%22;%20this.description%20=%20%22%22;%20this.element%20=%20document.createElement(%22button%22);%20this.element.classList.add(%22menu-hack%22);%20this.parent.append(this.element);%20%20if%20(name)%20this.setName(name);%20if%20(description)%20this.setDesc(description);%20}%20%20setName(name:%20string)%20{%20this.element.innerText%20=%20name;%20this.name%20=%20name;%20return%20this;%20}%20%20setClick(event:%20()%20=%3E%20unknown)%20{%20this.element.onclick%20=%20async%20()%20=%3E%20{%20await%20event();%20saveCharacter();%20console.log(`Triggered%20${this.name}.`);%20};%20return%20this;%20}%20%20setDesc(desc:%20string)%20{%20this.element.title%20=%20desc;%20this.description%20=%20desc;%20return%20this;%20}%20}
