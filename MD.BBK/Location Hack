javascript:// Location Hacks  // BEGIN IMPORTS import { category } from "../index"; // Import the Cheat GUI bases. import Toggler from "../class/Toggler"; import Hack from "../class/Hack"; import { Input, NumberInput, Swal, Toast } from "../utils/swal"; // Import Swal, Toast, and NumberInput from swal import { _, locations, prodigy, player } from "../utils/util"; // Import Prodigy typings import { toHouse } from "../utils/hackify"; // Import toHouse // END IMPORTS  export var useWASD : boolean = true;  // BEGIN LOCATION HACKS   // WASD Phasing new Toggler(category.location, "WASD Movement", "Allows you to walk through walls or on air with WASD movement in Prodigy.").setEnabled(async () => {     useWASD = true;     return Toast.fire("Enabled!", "WASD Movement is now enabled.", "success"); }).setDisabled(async () => {     useWASD = false;     return Toast.fire("Disabled!", "WASD Movement is now disabled.", "success"); }).status = true;   // Begin Edit walkSpeed new Hack(category.location, "Edit walkspeed", "Lets you set your walkspeed.").setClick(async () => {     const walkSpeed = await Input.fire("What do you want to set your walk speed to?%22);%20%20%20%20%20if%20(!walkSpeed.value)%20return;%20%20%20%20%20if%20(!player._playerContainer)%20{%20%20%20%20%20%20%20%20%20const%20interval%20=%20setInterval(()%20=%3E%20{%20%20%20%20%20%20%20%20%20%20%20%20%20if%20(player._playerContainer)%20{%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20clearInterval(interval);%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20player._playerContainer.walkSpeed%20=%20parseFloat(walkSpeed.value);%20%20%20%20%20%20%20%20%20%20%20%20%20}%20%20%20%20%20%20%20%20%20},%20100);%20%20%20%20%20}%20else%20player._playerContainer.walkSpeed%20=%20parseFloat(walkSpeed.value)%20||%201.5;%20%20%20%20%20return%20Toast.fire(%22Success!%22,%20`Successfully%20made%20walk%20speed%20${parseFloat(walkSpeed.value)%20||%201.5}!`,%20%22success%22);%20});%20//%20End%20Edit%20walkSpeed%20%20%20%20%20%20//%20Beg